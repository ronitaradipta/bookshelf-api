{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "quotes": [2, "double", { "avoidEscape": true }],
    "linebreak-style": "off",
    "no-console": "off"
  }
}
